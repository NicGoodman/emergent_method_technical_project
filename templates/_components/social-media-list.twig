<ul class="d-flex flex-row justify-content-center justify-content-sm-end w-100 list-unstyled mt-4">
	{% for block in entry.socialMediaAccounts %}
		{% set accountLink = block.accountLink %}
		{% if 'youtube' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="youtube" rel="noreferrer">
					<i class="fab fa-youtube"></i>
				</a>
			</li>
		{% elseif 'facebook' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="facebook" rel="noreferrer">
					<i class="fab fa-facebook-f"></i>
				</a>
			</li>
		{% elseif 'twitter' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="twitter" rel="noreferrer">
					<i class="fab fa-twitter"></i>
				</a>
			</li>
		{% elseif 'instagram' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="instagram" rel="noreferrer">
					<i class="fab fa-instagram"></i>
				</a>
			</li>
		{% elseif 'pinterest' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="pinterest" rel="noreferrer">
					<i class="fab fa-pinterest-p"></i>
				</a>
			</li>
		{% elseif 'linkedin' in accountLink %}
			<li>
				<a href="{{ accountLink }}" class="social-item mx-2 mx-sm-0 ms-0 ms-sm-4" target="_blank" rel="nofollow" aria-label="linkedin" rel="noreferrer">
					<i class="fab fa-linkedin-in"></i>
				</a>
			</li>
		{% endif %}
	{% endfor %}
</ul>
